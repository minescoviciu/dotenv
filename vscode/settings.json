{
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.tree.renderIndentGuides": "always",
    "catppuccin.accentColor": "sapphire",
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
    "vscode-neovim.logPath": "/tmp/vscode-nvim.log",
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
    "vscode-neovim.highlightGroups.highlights": {

        "IncSearch": {
            "backgroundColor": "theme.editor.findMatchBackground"
        },
        "CurSearch": {
            "backgroundColor": "theme.editor.findMatchBackground"
        },
        "Search": {
            "backgroundColor": "theme.editor.findMatchHighlightBackground"
        },
        "Substitute": {
            "backgroundColor": "theme.editor.foreground",
            "color": "theme.editor.background"
        }
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "git.blame.statusBarItem.template": "${subject},${authorName} (${authorDateAgo})",
    "git.blame.editorDecoration.enabled": true,
    "editor.renderWhitespace": "all",
    "vscode-neovim.compositeKeys": {
    },
    "redhat.telemetry.enabled": false,
    "workbench.navigationControl.enabled": false,
    "workbench.layoutControl.enabled": false,
    "lazygit-vscode.configPath": "~/.config/lazygit.yaml",
    "editor.fontSize": 15,
    "terminal.integrated.fontSize": 15,
    "window.zoomPerWindow": false,
    "window.zoomLevel": 1,
    "window.newWindowProfile": "Default",
    "workbench.iconTheme": "catppuccin-mocha",
    "terminal.integrated.profiles.osx": {
        "mine": {
            "path": "/bin/bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-tmux"
        }
    },
    "find-it-faster.general.batTheme": "Catppuccin Mocha",
    "find-it-faster.general.restoreFocusTerminal": true,
    "find-it-faster.general.showMaximizedTerminal": true,
    "find-it-faster.general.useGitIgnoreExcludes": false,
    "search.quickAccess.preserveInput": true,
    "search.smartCase": true,
    "search.useIgnoreFiles": false,
    "workbench.activityBar.location": "hidden",
    "workbench.sideBar.location": "right",
    "editor.find.seedSearchStringFromSelection": "never",
    "search.searchEditor.reusePriorSearchConfiguration": true,
}
